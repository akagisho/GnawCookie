{
    "name": "Gnaw Cookie",
    "description": "Gnaw Cookie allows you to get or set cookies easily.",
    "icons": {
        "16": "icon/icon_16.png",
        "48": "icon/icon_48.png",
        "128": "icon/icon_128.png"
    },
    "version": "1.0.0",
    "permissions": [
        "cookies",
        "tabs",
        "contextMenus",
        "http://*/*",
        "https://*/*"
    ],
    "app": {
        "launch": {
            "local_path": "get.html"
        }
    },
    "background": {
        "scripts": ["js/background.js"]
    },
    "manifest_version": 2
}
